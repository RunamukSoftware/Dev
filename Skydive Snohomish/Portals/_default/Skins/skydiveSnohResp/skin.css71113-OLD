/* fancy forms */
form.fancy fieldset {
	margin-bottom: 10px;
}
form.fancy legend {
	padding: 0 2px;
	font-weight: 700;
}
form.fancy label {
	display: inline-block;
	vertical-align: top;
	font-size: 13px;
	font-weight: 700;
}
form.fancy fieldset ol {
	margin: 0;
	padding: 0;
}
form.fancy fieldset li {
	list-style: none;
	padding: 5px;
	margin: 0;
}
form.fancy fieldset fieldset {
	border: none;
	margin: 3px 0 0;
}
form.fancy fieldset fieldset legend {
	padding: 0 0 5px;
	font-weight: normal;
}
form.fancy fieldset fieldset label {
	display: block;
	width: auto;
}
form.fancy em {
	font-weight: bold;
	font-style: normal;
	color: #f00;
}
form.fancy label {
	width: 220px; /* Width of labels */
}
form.fancy label.long {
	width: auto;
	display: block;
}
form.fancy fieldset fieldset label {
	margin-left: 223px; /* Width plus 3 (html space) */
}
form .invalid {
	border: 1px solid #fd214a;
}
label.invalid {
	display: none !important;
}
/* error, notice and success classes */
.error, .notice, .success {
	border: 2px solid #DDDDDD;
	margin-bottom: 1.6em;
	padding: 0.8em;
}
.error, .error a {
	background-color: #FBE3E4;
	border-color: #FBC2C4;
	color: #8A1F11;
}
.notice, .notice a {
	background-color: #FFF6BF;
	border-color: #FFD324;
	color: #514721;
}
.success, .success a {
	background-color: #E6EFC2;
	border-color: #C6D880;
	color: #264409;
}
.topbox {
	position: relative;
}
.topbox .caption {
	position: absolute;
	bottom: 0;
	padding: 5px;
	font-size: 13px;
	color: #fff;
	background-color: #000;
	display: block;
	width: 100%;
	z-index: 10;
}
.topbox {
	min-height: 430px;
}
#photo-video .topbox {
