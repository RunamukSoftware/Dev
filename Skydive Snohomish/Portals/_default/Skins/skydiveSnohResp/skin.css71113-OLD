/*    FORMS
    -------------------------------------------------------------------------------------------------------- */
input, textarea, select {
	background: #fff;
	border: 1px solid #aaa;
	box-shadow: 1px 1px 4px rgba(0, 0, 0, 0.2) inset;
	-moz-box-shadow: 1px 1px 4px rgba(0, 0, 0, 0.2) inset;
	-webkit-box-shadow: 1px 1px 4px rgba(0, 0, 0, 0.2) inset;
	font-size: 16px;
	padding: 5px;
}
textarea {
	font-family: "Helvetica", "Arial", sans-serif;
}
.ie input, .ie textarea, .ie select {
	color: #222;
}
label {
	display: block;
	font-size: 16px;
	margin-bottom: 3px;
}
label i {
	font-size: 12px;
}
label.error {
	color: #f00;
}
input[type=text]:focus, input.text:focus, input[type=password]:focus, select:focus, textarea:focus {
	color: #222;
	outline: 1px #353c37 solid;
}
hr {
	margin: 10px 0;
	color: #ccc;
	background-color: #ccc;
	height: 1px;
}
/* CLEAR FIX
    -------------------------------------------------------------------------------------------------------- */
.clearfix {
	display: inline-block;
}
html[xmlns] .clearfix {
	display: block;
}
.clearfix:after {
	clear: both;
	content: ".";
	display: block;
	height: 0;
	line-height: 0;
	visibility: hidden;
}
* html .clearfix {
	height: 1%;
}
/* IE6
    -------------------------------------------------------------------------------------------------------- */
.ie6 .pngfix {
	behavior: url(/css/iepngfix.htc);
}
.ie6 button, .ie7 button {
	width: auto;
	overflow: visible;
}
button::-moz-focus-inner {
	padding: 0;
	border: 0;
}
